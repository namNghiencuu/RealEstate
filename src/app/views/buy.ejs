        <div class="idcard">
          <p class="cmnd">Identification Card :</p>
          <input type="file" id="file" name="file" class="file aa" value="fileUpload" />
        </div>
      </div>
      <div class="edit">
        <button type="button" class="save">Book</button>
        <a href="../TeacherPhung/chooseProperty">
          <button type="button" class="save">Cancel</button></a>
      </div>
    </form>
