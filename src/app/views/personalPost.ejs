<!DOCTYPE html>
<html>

<head>
  <title>Real Estate Platform</title>
  <link rel="shortcut icon" href="/images/PageLogo/logo.png" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <link rel="stylesheet" href="/lib/fontawesome-free-5.8.1-web/css/all.min.css" />
  <link rel="stylesheet" type="text/css" href="stylesheets/modal.css" />
  <link rel="stylesheet" type="text/css" href="stylesheets/VerifyByService.css" />
  <link href="/font/EncodedSansExpanded.css" rel="stylesheet" />
  <link href="/font/OpenSansCondensed300.css" rel="stylesheet" />
</head>

<body>
  <section class="body1">
    <!--HEADER-->
    <div id="navbar">
      <div class="logo">
        <a href="./realestate"><img src="/images/PageLogo/logo_main.png" width="inherit;" /></a>
      </div>
      <div><a href="">Buy</a></div>
      <div><a href="">Rent</a></div>
      <div><a href="">Sell</a></div>
      <div><a href="">Agent Finder</a></div>
      <div><a href="/signin" class="abc">Sign in</a></div>
      <div><a href="" class="abc">Help</a></div>
    </div>
    <!--MAINPAGE-->
    <div class="mainmenu">
      <p class="titlethanhtoan">Các bài post của bản thân</p>
      <div class="tableinfo">
        <table style="width: 50%;" class="propertyinfo">
          <tr>
            <th>Tên bài post</th>
            <th>Giá</th>
            <th>Đường link</th>
            <th>Trạng thái</th>
            <th>Đăng bài</th>
          </tr>
          <% listPost.map((post, index)=>{ %>
          <tr>
            <td><%- post.title %></td>
            <td><%- post.price %></td>
            <td><a href="post/<%- post.id %>">Đường link</a></td>
            <td><%- post.status %></td>
            <td><a href="checkpackages?id=<%- post.id %>">Đăng bài</a></td>
          </tr>
          <%}) %>
        </table>
      </div>
    </div>
    <!--FOOTER-->
  </section>
</body>

</html>